{
  "name": "viking-ol",
  "version": "1.0.0",
  "main": "scr/dist/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc -d --emitDeclarationOnly --checkJs false --allowJs false --noImplicitAny false",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "clean": "rimraf dist lib",
    "test": "jest"
  },
  "repository": "git@github.com:phoenix-fullstack/viking-ol.git",
  "author": "zhaobaojue <zhaobaojue@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "ol": "^6.5.0",
    "ol-wind": "^1.0.0-alpha.10",
    "styled-components": "^5.2.1",
    "wind-core": "^1.0.0-alpha.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/ol": "^6.4.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "jest": "^26.6.3",
    "react-testing-library": "^8.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^4.1.3"
  }
}
